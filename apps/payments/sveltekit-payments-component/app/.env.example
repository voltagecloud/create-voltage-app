# Voltage API Configuration

# Your Voltage API key
PUBLIC_VOLTAGE_API_KEY=

# Your organization ID from the Voltage dashboard
PUBLIC_VOLTAGE_ORGANIZATION_ID=

# Your environment ID 
PUBLIC_VOLTAGE_ENVIRONMENT_ID=

# The wallet ID you want to receive payments to
PUBLIC_VOLTAGE_WALLET_ID=

# Optional: Custom base URL (defaults to https://voltageapi.com/v1)
PUBLIC_VOLTAGE_BASE_URL=https://voltageapi.com/v1